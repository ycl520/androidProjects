<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<LinearLayout android:orientation="vertical" android:id="@+id/lineartest"
		android:gravity="center_horizontal" android:layout_width="fill_parent"
		android:layout_alignParentTop="true" android:layout_height="wrap_content">
		<ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         />
	    
		<WebView android:layout_width="match_parent" android:id="@+id/seweb"
			android:layout_height="wrap_content" android:paddingBottom="10dip"></WebView>


	</LinearLayout>
	<LinearLayout android:orientation="vertical" android:id="@+id/seMenu"
		android:layout_alignParentBottom="true" android:layout_gravity="center"
		android:background="@drawable/tab_bar" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_centerInParent="true">

	</LinearLayout>
	<FrameLayout android:layout_height="wrap_content"

		android:layout_width="wrap_content" android:id="@+id/frameLayout1"
		android:foregroundGravity="center"
		android:layout_centerHorizontal="true"
		android:layout_centerInParent="true"
		
		android:layout_gravity="center">
		<ProgressBar android:id="@+id/progressBar1"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></ProgressBar>
	</FrameLayout>
</RelativeLayout>

		